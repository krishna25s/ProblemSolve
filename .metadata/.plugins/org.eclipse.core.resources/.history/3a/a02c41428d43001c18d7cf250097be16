/**
 * Add two number and if value is equal to target return indices
 * Algorithm:
 * 
 */


public class AddNum {

	public Integer addition() {
		int[] arr={7,8,5,6};
		for(int i=0; i<=arr.length-1;i++) {
			System.out.println(arr[i]);
		}
		return 1;
	}
	
	public static void main(String[] args) {
		AddNum addNum =new AddNum();
		addNum.addition();
	}

}
